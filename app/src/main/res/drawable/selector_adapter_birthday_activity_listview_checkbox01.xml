<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/dimen_5" />
            <stroke android:width="@dimen/dimen_2" android:color="@color/adapter_birthday_activity_listview_checkbox01" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/adapter_birthday_activity_listview_checkbox01" />
            <corners android:radius="@dimen/dimen_5" />
            <stroke android:width="@dimen/dimen_2" android:color="@color/adapter_birthday_activity_listview_checkbox01" />
        </shape>
    </item>
</selector>