<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:topLeftRadius="@dimen/dimen_5" android:bottomLeftRadius="@dimen/dimen_5" />
            <stroke android:width="@dimen/dimen_2" android:color="@color/include_new_perm_color" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/include_new_perm_color" />
            <corners android:topLeftRadius="@dimen/dimen_5" android:bottomLeftRadius="@dimen/dimen_5" />
            <stroke android:width="@dimen/dimen_2" android:color="@color/include_new_perm_color" />
        </shape>
    </item>
</selector>